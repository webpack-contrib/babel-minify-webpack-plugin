{
  "presets": [
    [
      "env",
      {
        "useBuiltIns": true,
        "targets": {
          "node": "6"
        },
        "exclude": [
          "transform-async-to-generator",
          "transform-regenerator"
        ]
      }
    ]
  ],
  "plugins": [
    [
      "transform-object-rest-spread",
      {
        "useBuiltIns": true
      }
    ]
  ],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "exclude": [
              "transform-regenerator"
            ],
            "include": [
              "transform-async-to-generator"
            ]
          }
        ]
      ],
      "plugins": [
        "transform-object-rest-spread"
      ]
    }
  }
}
